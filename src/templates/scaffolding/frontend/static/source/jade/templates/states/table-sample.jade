.row
    .col-lg-12
        .portlet.box
            .portlet-header
                .caption Customer Listing
                .actions
                    a.btn.btn-info.btn-xs(href='javascript:void(0);')
                        i.fa.fa-plus
                        | &nbsp;
                        | New Customer
                    | &nbsp;
                    .btn-group
                        a.btn.btn-warning.btn-xs.dropdown-toggle(href='javascript:void(0);', data-toggle='dropdown')
                            i.fa.fa-wrench
                            | &nbsp;
                            | Tools
                        ul.dropdown-menu.pull-right
                            li
                                a(href='javascript:void(0);')
                                    | Export to Excel
                            li
                                a(href='javascript:void(0);')
                                    | Export to CSV
                            li
                                a(href='javascript:void(0);')
                                    | Export to XML
                            li.divider
                            li
                                a(href='javascript:void(0);')
                                    | Print Invoices
            .portlet-body
                .row.mbm
                    .col-lg-12
                        .alert.alert-danger.tb-alert-error(style='display: none;')
                            button.close(type='button', data-dismiss='alert', aria-hidden='true') &times;
                            | Lorem ipsum dolor sit amet, consectetur adipiscing elit.
                        .alert.alert-success.tb-alert-success(style='display: none')
                            button.close(type='button', data-dismiss='alert', aria-hidden='true') &times;
                            | Lorem ipsum dolor sit amet, consectetur adipiscing elit.
                textarea.gw-row(style='display: none;')
                    tr
                        td {checkbox}
                        td {index}
                        td {name}
                        td {country}
                        td {gender}
                        td {order}
                        td {date}
                        td {price}
                        td {status}
                        td
                            button.btn.btn-default.btn-xs(type='button')
                                i.fa.fa-edit
                                | &nbsp;
                                | Edit
                            | &nbsp;
                            button.btn.btn-danger.btn-xs(type='button')
                                i.fa.fa-trash-o
                                | &nbsp;
                                | Delete
                .row.mbm
                    .col-lg-6
                        .pagination-panel
                            | Page
                            | &nbsp;
                            a.btn.btn-sm.btn-default.btn-prev.gw-prev(href='javascript:void(0);')
                                i.fa.fa-angle-left
                            | &nbsp;
                            input.pagination-panel-input.form-control.input-mini.input-inline.input-sm.text-center.gw-page(type='text', maxlenght='5', value='1')
                            | &nbsp;
                            a.btn.btn-sm.btn-default.btn-prev.gw-next(href='javascript:void(0);')
                                i.fa.fa-angle-right
                            | &nbsp;
                            | of 6 | View
                            | &nbsp;
                            select.form-control.input-xsmall.input-sm.input-inline.gw-pageSize
                                option(value='20', selected='selected') 20
                                option(value='50') 50
                                option(value='100') 100
                                option(value='150') 150
                                option(value='-1') All
                            | &nbsp;
                            | records | Found total 58 records
                    .col-lg-6
                        .tb-group-actions.pull-right
                            span 10 records selected:
                            select.table-group-action-select.form-control.input-inline.input-small.input-sm.mlm
                                option(value='0') Select...
                                option(value='1') Cancel
                                option(value='2') Hold
                                option(value='3') On Hold
                                option(value='4') Close
                            | &nbsp;
                            button.btn.btn-sm.btn-primary.submit-action
                                i.fa.fa-check
                                | &nbsp;
                                | Submit
