.row#demo-checkbox-radio
    .col-lg-12
        h3 Minimal skin
        .row.skin#minimal-skin
            .col-md-4
                h4.box-heading Live Checkbox
                ul.list-unstyled
                    li.pbm
                        input#minimal-checkbox-1(tabindex='5', type='checkbox')
                        | &nbsp;&nbsp;
                        label(for='minimal-checkbox-1') Checkbox 1
                    li.pbm
                        input#minimal-checkbox-2(tabindex='6', type='checkbox', checked='checked')
                        | &nbsp;&nbsp;
                        label(for='minimal-checkbox-2') Checkbox 2
                    li.pbm
                        input#minimal-checkbox-disabled(type='checkbox', disabled='disabled')
                        | &nbsp;&nbsp;
                        label(for='minimal-checkbox-disabled') Disabled
                    li.pbm
                        input#minimal-checkbox-disabled-checked(type='checkbox', checked='checked', disabled='disabled')
                        | &nbsp;&nbsp;
                        label(for='minimal-checkbox-disabled-checked') Disabled & checked
                .colors
                    strong Color schemes
                    ul.list-unstyled
                        li.active(title='Black')
                        li.red(title='Red')
                        li.green(title='Green')
                        li.blue(title='Blue')
                        li.aero(title='Aero')
                        li.grey(title='Grey')
                        li.orange(title='Orange')
                        li.yellow(title='Yellow')
                        li.pink(title='Pink')
                        li.purple(title='Purple')
            .col-md-4
                h4.box-heading Live Radio
                ul.list-unstyled
                    li.pbm
                        input#minimal-radio-1(tabindex='7', type='radio', name='minimal-radio')
                        | &nbsp;&nbsp;
                        label(for='minimal-radio-1') Radio button 1
                    li.pbm
                        input#minimal-radio-2(tabindex='8', type='radio', name='minimal-radio', checked='checked')
                        | &nbsp;&nbsp;
                        label(for='minimal-radio-2') Radio button 2
                    li.pbm
                        input#minimal-radio-disabled(type='radio', disabled='disabled')
                        | &nbsp;&nbsp;
                        label(for='minimal-radio-disabled') Disabled
                    li.pbm
                        input#minimal-radio-disabled-checked(type='radio', checked='checked', disabled='disabled')
                        | &nbsp;&nbsp;
                        label(for='minimal-radio-disabled-checked') Disabled & checked
            .col-md-4.skin-states
                h4.box-heading State
                ul.list-unstyled
                    li.pbm
                        .state.icheckbox_minimal
                        | &nbsp;&nbsp;
                        .state.iradio_minimal
                        | &nbsp;&nbsp;
                        label Normal
                    li.pbm
                        .state.icheckbox_minimal.hover
                        | &nbsp;&nbsp;
                        .state.iradio_minimal.hover
                        | &nbsp;&nbsp;
                        label Hover
                    li.pbm
